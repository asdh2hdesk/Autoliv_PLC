        <!-- QR Code Log Sequence -->
        <record id="seq_qr_code_log" model="ir.sequence">
            <field name="name">QR Code Log</field>
            <field name="code">qr.code.log</field>
            <field name="prefix">QR</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
        <!-- Serial Number Sequence for QR Code -->
        <record id="seq_plc_serial_number" model="ir.sequence">
            <field name="name">PLC Serial Number</field>
            <field name="code">plc.serial.number</field>
            <field name="prefix"></field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        
